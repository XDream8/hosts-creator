## defaults
syshosts_file=/etc/hosts
backupfilename=hosts
newhostsfn=hosts-new
downloader=curl
replacehosts=0

## remove unneeded lines
RM_COMMENTS=1
RM_DUPLICATE_LINES=1
RM_TRAILING_SPACES=1

## you should probably edit this
RESOLVE_HOST="127.0.0.1 $(hostname).homenetwork $(hostname) localhost"

